{
  "name": "cultuurnet/udb3-silex",
  "description": "UiTdatabank 3 core application",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "Kristof Coomans",
      "email": "kristof@2dotstwice.be"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^7.1",
    "2dotstwice/silex-feature-toggles-provider": "~0.1",
    "chrisboulton/php-resque": "dev-compat-1-2 as 1.2",
    "crell/api-problem": "~1.7",
    "cultuurnet/broadway-amqp": "~0.1",
    "cultuurnet/broadway-tools": "~0.1",
    "cultuurnet/cdb": "~2.2.0",
    "cultuurnet/culturefeed-php": "~1.9.2",
    "cultuurnet/hydra": "~0.1",
    "cultuurnet/monolog-socketio": "~0.1",
    "cultuurnet/search-v3": "dev-master",
    "cultuurnet/silex-amqp": "~0.1",
    "cultuurnet/silex-service-provider-jwt": "~0.1",
    "cultuurnet/udb2-domain-events": "~0.1",
    "cultuurnet/udb3": "~0.1",
    "cultuurnet/udb3-api-guard": "dev-master#205a569b46e0d59485e86eaa93f0db50e3fd96ce as 0.1",
    "cultuurnet/udb3-doctrine": "~0.1",
    "cultuurnet/udb3-export": "~0.1",
    "cultuurnet/udb3-http-foundation": "~0.1",
    "cultuurnet/udb3-models": "~0.1",
    "cultuurnet/udb3-models-import": "~0.1",
    "cultuurnet/udb3-symfony": "~0.1",
    "cultuurnet/udb3-udb2-bridge": "~0.1",
    "cultuurnet/udb3-uitpas-bridge": "~0.1",
    "cultuurnet/uitid-credentials": "~0.1",
    "deralex/yaml-config-service-provider": "~1.0",
    "doctrine/dbal": "~2.4",
    "doctrine/migrations": "~1.0@dev",
    "hanneskod/classtools": "~1.0",
    "jdesrosiers/silex-cors-provider": "~0.1.4",
    "jeremykendall/php-domain-parser": "4.0.3-alpha as 1.3.1",
    "knplabs/console-service-provider": "~1.0",
    "league/csv": "^9.1",
    "monolog/monolog": "~1.11",
    "nesbot/carbon": "^1.22",
    "php-http/guzzle6-adapter": "^1.0",
    "predis/predis": "~1.0",
    "rase/socket.io-emitter": "0.6.1",
    "silex/silex": "~1.2",
    "swiftmailer/swiftmailer": "~5.3",
    "twig/extensions": "^1.5",
    "willdurand/geocoder": "~2.8"
  },
  "require-dev": {
    "escapestudios/symfony2-coding-standard": "~2.9",
    "phing/phing": "~2.10",
    "phpunit/phpunit": "^7.5",
    "squizlabs/php_codesniffer": "~2.2"
  },
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "CultuurNet\\UDB3\\Silex\\": ["app/", "src/"]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CultuurNet\\UDB3\\Silex\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  },
  "config": {
    "preferred-install": {
      "cultuurnet/*": "source",
      "2dotstwice/*": "source",
      "*": "dist"
    },
    "sort-packages": true
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/cultuurnet/php-resque.git"
    },
    {
      "type": "path",
      "url": "lib/udb3-export",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "lib/udb3-udb2-bridge",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "lib/udb3-models-import",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "lib/udb3-symfony",
      "options": {
        "symlink": true
      }
    }
  ]
}
