{
  "name": "cultuurnet/udb3-silex",
  "description": "Silex UDB3 search platform",
  "license": "GPLv3",
  "authors": [
    {
      "name": "Kristof Coomans",
      "email": "kristof@2dotstwice.be"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "cultuurnet/silex-uitid-provider": "0.x-dev",
    "cultuurnet/entry": "~0.1",
    "cultuurnet/udb2-domain-events": "~0.1",
    "cultuurnet/udb3": "dev-feature/III-1034 as 0.1",
    "cultuurnet/udb3-doctrine": "~0.1",
    "cultuurnet/udb3-symfony": "dev-feature/III-1034 as 0.1",
    "cultuurnet/udb3-udb2-bridge": "~0.1",
    "cultuurnet/cdb": "~2.2.0",
    "cultuurnet/udb3-silex-entryapi": "~0.1",
    "cultuurnet/hydra": "~0.1",
    "silex/silex": "~1.2",
    "deralex/yaml-config-service-provider": "~1.0",
    "monolog/monolog": "~1.11",
    "chrisboulton/php-resque": "~1.2",
    "doctrine/dbal": "~2.4",
    "predis/predis": "~1.0",
    "rase/socket.io-emitter": "0.6.1",
    "jdesrosiers/silex-cors-provider": "~0.1.4",
    "swiftmailer/swiftmailer": "~5.3",
    "knplabs/console-service-provider": "~1.0",
    "doctrine/migrations": "~1.0@dev",
    "crell/api-problem": "~1.7",
    "cultuurnet/silex-service-provider-oauth": "~0.1",
    "2dotstwice/silex-feature-toggles-provider": "~0.1",
    "php-http/guzzle6-adapter": "^1.0",
    "cultuurnet/broadway-amqp": "~0.1",
    "cultuurnet/silex-amqp": "~0.1",
    "cultuurnet/silex-service-provider-jwt": "~0.1",
    "cultuurnet/uitid-credentials": "~0.1"
  },
  "require-dev": {
    "escapestudios/symfony2-coding-standard": "~2.9",
    "squizlabs/php_codesniffer": "~2.2",
    "phing/phing": "~2.10"
  },
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "CultuurNet\\UDB3\\Silex\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  },
  "config": {
    "preferred-install": {
      "cultuurnet/*": "source",
      "2dotstwice/*": "source",
      "*": "dist"
    },
    "platform": {
      "php": "5.5.9",
      "ext-redis": "2.2.7.0",
      "ext-intl": "1.1.0"
    }
  }
}
