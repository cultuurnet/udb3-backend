{
  "name": "cultuurnet/udb3-silex",
  "description": "UiTdatabank 3 core application",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "Publiq vzw",
      "email": "info@publiq.be"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.4",
    "ext-dom": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-pcntl": "*",
    "ext-pdo": "*",
    "ext-redis": "*",
    "ext-simplexml": "*",
    "ext-tidy": "*",
    "ext-xmlreader": "*",
    "auth0/auth0-php": "^8.2",
    "broadway/broadway": "^2.4",
    "cakephp/chronos": "^1.3",
    "chrisboulton/php-resque": "dev-compat-1-2 as 1.2",
    "commerceguys/intl": "^0.7",
    "cultuurnet/calendar-summary-v3": "^3.4",
    "cultuurnet/cdb": "~2.2.0",
    "cultuurnet/culturefeed-php": "dev-master",
    "cultuurnet/udb3-api-guard": "^5.0",
    "danielstjules/stringy": "^3.1",
    "doctrine/cache": "~1.3",
    "doctrine/dbal": "~2.4",
    "doctrine/migrations": "~1.0@dev",
    "galbar/jsonpath": "^2.0",
    "geocoder-php/google-maps-provider": "^4.6",
    "gridonic/princexml-php": "~1.1.1",
    "guzzlehttp/guzzle": "^7.4",
    "guzzlehttp/psr7": "^2.4",
    "jeremykendall/php-domain-parser": "4.0.3-alpha as 1.3.1",
    "laminas/laminas-httphandlerrunner": "^2.2",
    "lcobucci/jwt": "3.3.*",
    "league/container": "^4.2",
    "league/csv": "^9.1",
    "league/flysystem": "^2.2.3",
    "league/flysystem-aws-s3-v3": "^2.1",
    "league/period": "^3.3",
    "league/route": "^5.1",
    "league/uri": "^6.3",
    "league/uri-components": "^2.4",
    "mathiasverraes/money": "^v1.3.0",
    "monolog/monolog": "~1.11",
    "nesbot/carbon": "^1.22",
    "opis/json-schema": "^2.1",
    "php-amqplib/php-amqplib": "^2.12",
    "php-http/guzzle7-adapter": "^1.0",
    "php-http/httplug": "^2.3",
    "phpoffice/phpspreadsheet": "^1.18",
    "predis/predis": "~1.0",
    "psr/http-server-middleware": "^1.0",
    "psr/log": "^1.0",
    "publiq/udb3-json-schemas": "dev-main",
    "ramsey/uuid": "^3.2.0",
    "rase/socket.io-emitter": "0.6.1",
    "sentry/sentry": "^3.6",
    "silex/silex": "^1.3",
    "slim/psr7": "^1.4",
    "swiftmailer/swiftmailer": "~5.3",
    "symfony/serializer": "^v3.1.10",
    "twig/extensions": "^1.5",
    "twig/twig": "~1.0",
    "webmozart/assert": "^1.2",
    "willdurand/geocoder": "^4.5",
    "willdurand/negotiation": "^3.0",
    "zendframework/zend-diactoros": "^1.3"
  },
  "require-dev": {
    "alchemy/zippy": "~0.2",
    "icanhazstring/composer-unused": "^0.7.7",
    "mikey179/vfsstream": "~1.6.2",
    "phpstan/phpstan": "^1.8",
    "phpunit/phpunit": "^8.5",
    "publiq/php-cs-fixer-config": "^1.3",
    "rector/rector": "^0.14.5"
  },
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "CultuurNet\\UDB3\\": ["app/", "src/"]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CultuurNet\\UDB3\\": "tests//"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  },
  "config": {
    "allow-plugins": {
      "kylekatarnls/update-helper": true,
      "icanhazstring/composer-unused": true
    },
    "preferred-install": {
      "cultuurnet/*": "source",
      "2dotstwice/*": "source",
      "*": "dist"
    },
    "sort-packages": true
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/cultuurnet/php-resque.git"
    }
  ],
  "scripts": {
    "test": "vendor/bin/phpunit",
    "phpstan": "vendor/bin/phpstan analyse",
    "migrate": "vendor/bin/doctrine-dbal migrations:migrate --no-interaction",
    "ci": [
      "composer phpstan",
      "composer test",
      "composer cs"
    ],
    "cs": "php-cs-fixer fix -v --diff --dry-run",
    "cs-fix": "php-cs-fixer fix -v --diff"
  }
}
