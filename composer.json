{
  "name": "cultuurnet/udb3-silex",
  "description": "UiTdatabank 3 core application",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "Publiq vzw",
      "email": "info@publiq.be"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.4",
    "ext-dom": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-pcntl": "*",
    "ext-pdo": "*",
    "ext-redis": "*",
    "ext-simplexml": "*",
    "ext-tidy": "*",
    "ext-xmlreader": "*",
    "auth0/auth0-php": "^5.7",
    "broadway/broadway": "1.0.0",
    "cakephp/chronos": "^1.3",
    "chrisboulton/php-resque": "dev-compat-1-2 as 1.2",
    "commerceguys/intl": "^0.7",
    "cultuurnet/calendar-summary-v3": "^3.4",
    "cultuurnet/cdb": "~2.2.0",
    "cultuurnet/culturefeed-php": "dev-master",
    "cultuurnet/udb3-api-guard": "^v4.0",
    "cultuurnet/valueobjects": "^4.1",
    "danielstjules/stringy": "^3.1",
    "doctrine/cache": "~1.3",
    "doctrine/dbal": "~2.4",
    "doctrine/migrations": "~1.0@dev",
    "gridonic/princexml-php": "~1.1.1",
    "guzzlehttp/guzzle": "^6.3",
    "guzzlehttp/psr7": "^1.4",
    "hanneskod/classtools": "~1.0",
    "jdesrosiers/silex-cors-provider": "~0.1.4",
    "jeremykendall/php-domain-parser": "4.0.3-alpha as 1.3.1",
    "knplabs/console-service-provider": "~1.0",
    "lcobucci/jwt": "3.3.*",
    "league/csv": "^9.1",
    "league/flysystem": "^2.2.3",
    "league/flysystem-aws-s3-v3": "^2.1",
    "league/flysystem-ziparchive": "^2.0.6",
    "league/period": "^3.3",
    "league/uri": "~4.2",
    "marvin_b8/psr-7-service-provider": "^1.0",
    "monolog/monolog": "~1.11",
    "nesbot/carbon": "^1.22",
    "opis/json-schema": "^2.1",
    "php-amqplib/php-amqplib": "^2.12",
    "php-http/guzzle6-adapter": "^1.0",
    "php-http/httplug": "^1.0.0",
    "phpoffice/phpspreadsheet": "^1.18",
    "predis/predis": "~1.0",
    "psr/http-server-middleware": "^1.0",
    "psr/log": "dev-master",
    "publiq/stoplight-docs-uitdatabank": "dev-booking-availability",
    "rase/socket.io-emitter": "0.6.1",
    "respect/validation": "~1.1",
    "sentry/sdk": "^2.2",
    "silex/silex": "^1.3",
    "slim/psr7": "^1.4",
    "swiftmailer/swiftmailer": "~5.3",
    "symfony/http-foundation": "~2.8.52",
    "symfony/http-kernel": "~2.3",
    "symfony/process": "~3.0",
    "symfony/psr-http-message-bridge": "~0.2.0",
    "symfony/routing": "~2.3",
    "symfony/security": "~2.8.52",
    "symfony/yaml": "^4.4",
    "twig/extensions": "^1.5",
    "twig/twig": "~1.0",
    "webmozart/assert": "^1.2",
    "willdurand/geocoder": "^3.0",
    "zendframework/zend-diactoros": "^1.3"
  },
  "require-dev": {
    "alchemy/zippy": "~0.2",
    "mikey179/vfsstream": "~1.6.2",
    "phpstan/phpstan": "^0.12.42",
    "phpunit/phpunit": "^7.5",
    "publiq/php-cs-fixer-config": "^1.3"
  },
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "CultuurNet\\UDB3\\Silex\\": "app/",
      "CultuurNet\\UDB3\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CultuurNet\\UDB3\\Silex\\": "tests/",
      "CultuurNet\\UDB3\\": "tests//"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  },
  "config": {
    "preferred-install": {
      "cultuurnet/*": "source",
      "2dotstwice/*": "source",
      "*": "dist"
    },
    "sort-packages": true
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/cultuurnet/php-resque.git"
    }
  ],
  "scripts": {
    "test": "vendor/bin/phpunit",
    "phpstan": "vendor/bin/phpstan analyse",
    "migrate": "vendor/bin/doctrine-dbal migrations:migrate --no-interaction",
    "ci": [
      "composer phpstan",
      "composer test",
      "composer cs"
    ],
    "cs": "php-cs-fixer fix -v --diff --dry-run",
    "cs-fix": "php-cs-fixer fix -v --diff"
  }
}
