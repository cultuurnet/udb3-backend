<http://data.uitdatabank.local:80/events/%{eventId}>
  a cidoc:E7_Activity ;
  dcterms:created ""^^xsd:dateTime ;
  dcterms:modified ""^^xsd:dateTime ;
  adms:identifier [
    a adms:Identifier ;
    skos:notation "http://data.uitdatabank.local:80/events/%{eventId}"^^xsd:anyURI ;
