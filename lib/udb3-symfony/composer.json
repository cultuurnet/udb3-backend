{
  "name": "cultuurnet/udb3-symfony",
  "description": "UDB3 integration with Symfony HTTPFoundation",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "Kristof Coomans",
      "email": "kristof@2dotstwice.be"
    },
    {
      "name": "Bert Ramakers",
      "email": "bert@2dotstwice.be"
    }
  ],
  "require": {
    "php": ">=7.1",
    "ext-json": "*",
    "cultuurnet/hydra": "~0.1",
    "cultuurnet/udb3": "~0.1",
    "cultuurnet/udb3-api-guard": "~0.1",
    "cultuurnet/udb3-models": "~0.1",
    "cultuurnet/udb3-models-import": "~0.1",
    "cultuurnet/valueobjects": "~3.0",
    "crell/api-problem": "~1.7",
    "symfony/http-foundation": "~2.7.2",
    "symfony/http-kernel": "~2.3",
    "symfony/routing": "~2.3",
    "symfony/psr-http-message-bridge": "~0.2.0",
    "guzzlehttp/psr7": "~1.2.3",
    "guzzlehttp/guzzle": "~6.2",
    "zendframework/zend-diactoros": "^1.3",
    "chrisboulton/php-resque": "1.2",
    "cultuurnet/calendar-summary-v3": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7",
    "squizlabs/php_codesniffer": "~2.3",
    "phing/phing": "~2.11",
    "satooshi/php-coveralls": "~0.7"
  },
  "autoload": {
    "psr-4": {
      "CultuurNet\\UDB3\\Symfony\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CultuurNet\\UDB3\\Symfony\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  }
}
