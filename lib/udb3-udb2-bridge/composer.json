{
  "name": "cultuurnet/udb3-udb2-bridge",
  "description": "PHP libraries for bridging UDB3 with UDB2",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Kristof Coomans",
      "email": "kristof@2dotstwice.be"
    },
    {
      "name": "Bram Cordie",
      "email": "bram@2dotstwice.be"
    }
  ],
  "require": {
    "cultuurnet/udb3": "~0.1",
    "cultuurnet/udb2-domain-events": "~0.1",
    "cultuurnet/auth": "~1.2",
    "cultuurnet/entry": "~0.1",
    "cultuurnet/cdb": "~2.2.0",
    "php-http/httplug": "~1.0.0",
    "guzzlehttp/psr7": "~1.2.3",
    "league/uri": "^4.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7",
    "squizlabs/php_codesniffer": "2.6.1",
    "phing/phing": "~2.10",
    "satooshi/php-coveralls": "~0.7"
  },
  "autoload": {
    "psr-4": {
      "CultuurNet\\UDB3\\UDB2\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CultuurNet\\UDB3\\UDB2\\": "test/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  }
}
