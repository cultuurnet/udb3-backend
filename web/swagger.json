        "/api/1.0/user": {
            "get": {
                "summary": "Retrieve all information about the current user.",
                "description": "Authentication is required.",
                "operationId": "getUserInformation",
                "produces": [
                    "application/json"
                ],
                "responses": {
                    "200": {
