        },
        "/event/{cdbid}/labels": {
            "post": {
                "summary": "Add a label to a single cultural event.",
                "description": "Authentication is required.",
                "operationId": "addLabelToEvent",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "cdbid",
                        "in": "path",
                        "description": "The unique identifier of the cultural event.",
                        "required": true,
                        "type": "string",
                        "format": "uuid"
                    },
                    {
                        "name": "body",
                        "in": "body",
                        "description": "The label to attach to the cultural event.",
