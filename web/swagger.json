                    {
                        "name": "body",
                        "in": "body",
                        "description": "A",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/exportRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Info on the command that was queued.",
                        "schema": {
                            "$ref": "#/definitions/command"
                        }
                    }
                },
                "tags": [
                    "Export",
                    "Event"
                ]
            }
        }
