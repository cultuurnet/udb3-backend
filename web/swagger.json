                "responses": {
                    "200": {
                        "description": "Info on the command that was queued.",
                        "schema": {
                            "$ref": "#/definitions/command"
                        }
                    }
                },
                "tags": [
                    "Saved search"
                ]
            }
        },
        "/events/export/{type}": {
            "post": {
                "summary": "Request to generate an event list exported to a file.",
