                ],
                "responses": {
                    "200": {
                        "description": "A list of labels recently used by the current user."
                    }
                },
                "tags": [
                    "User"
                ]
            }
        },
        "/api/1.0/search": {
            "get": {
                "summary": "Retrieve events for a given search query with a given offset.",
                "description": "Authentication is required.",
                "operationId": "apiSearch",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "query",
                        "in": "query",
                        "description": "The search query to execute.",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "name": "offset",
                        "in": "query",
                        "description": "The number of events to skip from the start of the result set.",
                        "required": false,
                        "type": "integer",
                        "format": "int32"
                    },
                    {
                      "name": "sort",
                      "in": "sort",
                      "description": "How to order the search results.\n\nSpecify a field and an optional direction (asc or desc) separated by a space.\n\nFor example: `lastupdated asc`",
                      "required": false,
                      "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "A list of events",
                        "schema": {
                            "$ref": "#/definitions/SearchResult"
                        }
                    }
                },
                "tags": [
                    "Search"
                ]
            }
        },
        "/command/{token}": {
            "get": {
                "summary": "View the staus of a an executed command.",
                "description": "Authentication is required.",
                "operationId": "getCommandStatus",
                "produces": [
                    "text/plain; charset=utf-8"
                ],
                "parameters": [
                    {
                        "name": "token",
                        "in": "path",
                        "description": "The unique token (commandId) of a command as returned from POST or DELETE actions.",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "The command status.",
                        "schema": {
                            "type": "string",
                            "enum": [
                                "waiting",
                                "running",
                                "complete",
                                "failed"
                            ]
                        }
                    }
                },
                "tags": [
                    "Command"
                ]
            }
        },
