                "responses": {
                    "200": {
                        "description": "Info on the command that was queued.",
                        "schema": {
                            "$ref": "#/definitions/command"
                        }
                    }
                },
                "tags": [
                    "Event"
                ]
            }
        },
        "/event/{cdbid}/{lang}/description": {
            "post": {
                "summary": "Translate the description of a cultural event.",
