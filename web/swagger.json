        "/place/{cdbid}": {
            "get": {
                "summary": "View the details of a specific place.",
                "operationId": "getPlace",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "cdbid",
                        "in": "path",
                        "description": "The unique identifier of the place.",
                        "required": true,
